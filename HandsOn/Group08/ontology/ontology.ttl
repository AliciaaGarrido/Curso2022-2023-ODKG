@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix ns: <https://www.healthfinder.io/group08/resource/namespace#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<https://www.healthfinder.io/group08/ontology> rdf:type owl:Ontology .

geo: a owl:Ontology ;
    dc:creator "Alejandro Macari",
        "Andrea Pisani",
        "Anne-Fleur Kerhousse",
        "Ariel Ratzonel",
        "Natalia Bagnoli" ;
    dc:description "Ontology code created by Chowlk" ;
    dc:title "HealthFinder Ontology" ;
    owl:versionInfo "0.0.1" .

#################################################################
#    Object Properties
#################################################################

geo:latitude a owl:DatatypeProperty ;
    rdfs:label "latitude" .

geo:location a owl:ObjectProperty ;
    rdfs:label "location" ;
    rdfs:domain ns:Establishment ;
    rdfs:range ns:Address .

geo:longitude a owl:DatatypeProperty ;
    rdfs:label "longitude" .

ns:belongTo a owl:ObjectProperty ;
    rdfs:label "belong to" ;
    rdfs:domain ns:Address ;
    rdfs:range ns:Municipality .

ns:isIn a owl:ObjectProperty ;
    rdfs:label "is in" ;
    rdfs:domain ns:Clinic,
        ns:Defibrillator ;
    rdfs:range ns:Establishment .

#################################################################
#    Data properties
#################################################################

ns:openingHours a owl:DatatypeProperty ;
    rdfs:label "opening hours" .

ns:ownership a owl:DatatypeProperty ;
    rdfs:label "ownership" .

ns:specialization a owl:DatatypeProperty ;
    rdfs:label "specialization" .

ns:tipology a owl:DatatypeProperty ;
    rdfs:label "typology" .

ns:tipologyOfClinic a owl:DatatypeProperty ;
    rdfs:label "tipology of clinic" .

#################################################################
#    Classes
#################################################################

ns:Clinic a owl:Class ;
    rdfs:label "Clinic" .

ns:Defibrillator a owl:Class ;
    rdfs:label "Defibrillator" .

ns:Municipality a owl:Class ;
    rdfs:label "Municipality" .

ns:Address a owl:Class ;
    rdfs:label "Address" .

ns:Establishment a owl:Class ;
    rdfs:label "Establishment" .

